#ifndef INC_SPWM_H_
#define INC_SPWM_H_

typedef enum
{
	OFF = 0,
	 ON = 1,

}eSwitch_Type;

typedef struct
{
	uint32_t Prescaler;
	uint32_t Period;
	uint32_t Pulse;
}Freq_HandleTypeDef;
uint32_t const FCPU = 64000000;
uint16_t const MAX16BIT = 65535;


#endif /* INC_SPWM_H_ */
